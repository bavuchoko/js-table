{
  "name": "@bavuchoko/js-table",
  "version": "1.0.19",
  "description": "A customizable React table component with resizing, reordering, and pagination.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "buildm": "tsc && cp src/index.css dist/index.css",
    "buildw": "tsc && powershell -Command \"Copy-Item src/index.css dist/index.css\""
  },
  "keywords": [
    "react",
    "table",
    "resizable",
    "draggable",
    "pagination",
    "typescript",
    "component"
  ],
  "author": "bellwater1985",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "typescript": "^4.9.5"
  }
}
